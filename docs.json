{
  "name": "developers.eos.io",
  "generators": [
    {
      "name": "collate_markdown",
      "options": {
        "docs_dir": "docs"
      }
    },
    {
      "name": "glossary",
      "options": {
        "glossary_markdown_output": "glossary",
        "glossary_source": "docs/glossary.yaml"
      }
    }
  ],
  "children": [
    { "repo": "eosio.cdt", "path": "manuals/eosio.cdt", "tagged": "true" },
    { "repo": "eosio.contracts", "path": "manuals/eosio.contracts", "tagged": "true" },
    { "repo": "eosjs", "path": "manuals/eosjs", "tagged": "true" },
    { "repo": "eos", "path": "manuals/eosio", "tagged": "true" }
  ]
}
